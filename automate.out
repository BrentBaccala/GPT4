#!/usr/bin/env python3

import requests
from bs4 import BeautifulSoup
from selenium import webdriver
from selenium.webdriver.common.keys import Keys

url = "https://poe.com/ChatGPT"

# Set up the webdriver (change the path to your chromedriver location)
driver = webdriver.Chrome(executable_path="/path/to/chromedriver")

# Authenticate, if needed
# driver.find_element_by_id("username").send_keys("your_username")
# driver.find_element_by_id("password").send_keys("your_password")
# driver.find_element_by_id("login_button").click()

# Load the HTML
driver.get(url)

# Find the textarea in the DOM
textarea = driver.find_element_by_tag_name("textarea")

# Fill in the textarea with the query
query = "what is the windows cli command to see the current wifi network?"
textarea.send_keys(query)

# Push the button
button = driver.find_element_by_tag_name("button")
button.click()

# Close the driver when done
driver.quit()
