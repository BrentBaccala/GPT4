import requests
from bs4 import BeautifulSoup
from selenium import webdriver
from selenium.webdriver.common.keys import Keys
import time

url = 'https://poe.com/ChatGPT'

# Set up a WebDriver instance (e.g., ChromeDriver)
driver = webdriver.Chrome(executable_path='chromedriver_path')
driver.get(url)
time.sleep(2)

# Authenticate if necessary
# Replace 'your_username' and 'your_password' with your actual credentials
username = 'your_username'
password = 'your_password'
username_field = driver.find_element_by_name("username")
password_field = driver.find_element_by_name("password")
username_field.send_keys(username)
password_field.send_keys(password)
password_field.send_keys(Keys.RETURN)
time.sleep(2)

# Find the textarea element
textarea = driver.find_element_by_tag_name("textarea")

# Fill in the textarea with the query
query = "what is the windows cli command to see the current wifi network?"
textarea.send_keys(query)

# Find and click the button
button = driver.find_element_by_tag_name("button")
button.click()

# Wait for the response
time.sleep(5)

# Find the response container
response_container = driver.find_element_by_class_name("response-container")

# Print the response
print(response_container.text)

# Close the browser window
driver.quit()
