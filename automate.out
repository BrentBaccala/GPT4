#!/usr/bin/env python3

import time
from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By
from selenium.webdriver.support.ui import WebDriverWait
from selenium.webdriver.support import expected_conditions as EC

url = 'https://poe.com/ChatGPT'
query = 'what is the windows cli command to see the current wifi network?'

# Set up the WebDriver
driver = webdriver.Firefox(executable_path='./geckodriver')

# Navigate to the URL
driver.get(url)

# Log in
username = 'your_username'
password = 'your_password'
username_field = driver.find_element_by_name('username')
password_field = driver.find_element_by_name('password')
username_field.send_keys(username)
password_field.send_keys(password)
password_field.send_keys(Keys.RETURN)

# Wait for the textarea to be available
wait = WebDriverWait(driver, 10)
textarea = wait.until(EC.presence_of_element_located((By.TAG_NAME, 'textarea')))

# Fill in the textarea with the query
textarea.clear()
textarea.send_keys(query)

# Push the button
button = driver.find_element_by_css_selector("button[type='submit']")
button.click()

# Wait for a while to let the response load
time.sleep(5)

# Close the browser window
driver.quit()
